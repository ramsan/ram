#signInUpBox
  #xGoBack= link_to "X", root_path, :id => "Cancel"
  #DYRlogo
    = image_tag "doYouRememberSmallLogo.png", :title => "Do Your Remember?"
  .loginUsingSocialNetworks
    = link_to image_tag("facebook_login_bar.png", :title => "Login Using Your Facebook Account"), '/auth/facebook', :class => "social"
    = link_to image_tag("twitter_login_bar.png", :title => "Login Using Your Twitter Account"), '/auth/twitter', :class => "social"
  #loginManually
    = form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f|
      .field
        = f.email_field :email, :placeholder => "Login with Email"
      .field
        = f.password_field :password, :placeholder => "Password"
      .field
        = f.submit "Sign in", :class => "greyButton"
        .right
          = link_to "Forgot password?", new_password_path(resource_name)

  .clear
  #signUpLink
    Sign Up with Email
    = link_to "Click Here", new_registration_path(resource_name)

%h1 Coney Island Beach,  NYC  Circa 1938  
